<h2>All Produce</h2>
<table class="table table-striped table-bordered">
  <thead class="table-header">
    <tr>
      <th scope="col">Produce Name</th>
    </tr>
  </thead>
  <tbody>
    <% @produces.each do |produce| %>
      <tr>
        <td><%= link_to produce.produce_name, produce_path(produce), class: "btn btn-info btn-sm" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "Add Produce ", new_produce_path %>
