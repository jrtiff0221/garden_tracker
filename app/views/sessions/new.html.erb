<h1> Sign Up Here! </h1>

<%= form_for (@user) do |f| %>
    <ul>
    <% @user.errors.full_messages.each do |error_message|%>
    <li><%= error_message%></li>
    <%end%>
    </ul>

    <%= f.label :username %>
    <%= f.text_field :username %>
<br>
    <%= f.label :email %>
    <%= f.email_field :email %>
<br>
    <%= f.label :password %>
    <%= f.password_field :password %>
<br>
    <%=  f.submit "Sign Up" %>
<%end%>